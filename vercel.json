{
  "builds": [{
      "src": "api/wsgi.py",
      "use": "@vercel/python",
      "config": { "maxLambdaSize": "15mb", "runtime": "python3.9" }
  }],

  "routes": [
    {
      "src": "/(.*)",
      "dest": "api/wsgi.py"
    },
    {
      "src": "/static/(.*)",
      "dest": "/staticfiles/$1"
    },
    {
      "src": "/media/(.*)",
      "dest": "/mediafiles/$1"
    }
  ],
  "outputDirectory": "staticfiles"
}
